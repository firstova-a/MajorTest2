@{
}
@using Laboratory9.Models
@model LoginModel
<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Регистрация</title>
</head>
<body>
    <h1>Регистрация</h1>


    <a href="/Home/Index">На главную</a>
    <p>
        @Model.ErrorMessage
    </p>
    <form method="post">
        <p>Логин</p>
        <input type="text" name="login" value="@Model.Login" />
        <p>Пароль</p>
        <input type="password" name="pswd" value="@Model.Password" />
        <input type="submit" value="Зарегистрироваться" />
    </form>

</body>
</html>
